bs_package_flags = -bs-package-name bs-demo -bs-package-map BsDemo
src_root_dir = /Users/davidsancho/dev/reason-console-formatter/test/bs-demo
bsc = /Users/davidsancho/.config/yarn/global/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/davidsancho/.config/yarn/global/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/davidsancho/dev/reason-console-formatter/test/bs-demo/node_modules/bs-platform/lib/ocaml' -bs-g -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = -ns BsDemo
bsb_dir_group = 0
refmt = /Users/davidsancho/.config/yarn/global/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/Demo.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo.mlast.d : build_deps src/Demo.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo-BsDemo.cmj |  $src_root_dir/src/Demo.bs.js src/Demo-BsDemo.cmi : build_cmj_cmi src/Demo.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output es6-global:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -bs-cmi-only -c ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build BsDemo.cmi : build_package BsDemo.mlmap
build build.ninja : phony  ||  BsDemo.cmi src/Demo.mlast.d
